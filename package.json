  "scripts": {
    "clean": "rm -rf ./dist/*",
    "premili:upgrade": "npm install mili@latest -D",
    "mili:upgrade": "mili upgrade",
    "build": "npm run clean & webpack",
    "build:prod": "NODE_ENV=production npm run build",
    "start": "node build/engine.js",
    "prestart:port": "NODE_ENV=production npm run build",
    "start:prod": "NODE_ENV=production node dist/server/bundle.js",
    "setup": "node run setup:stage",
    "setup:stage": "pm2 deploy ecosystem.config.js stage setup",
    "setup:prod": "pm2 deploy ecosystem.config.js prod setup",
    "deploy": "npm run deploy:stage",
    "deploy:stage": "pm2 deploy ecosystem.config.js stage",
    "deploy:prod": "pm2 deploy ecosystem.config.js prod",
    "analyzer": "NODE_ENV=production ANALYZER=true webpack"
  },
  "engines": {
    "node": ">=8.9.4"
  },
  "deploy": {
    "user": "",
    "host": "",
    "path": "",
    "prod": {},
    "dev": {}
  },
  "mili": {
    "type": "pwa",
    "encoding": "utf8"
  },
  "devDependencies": {
    "mili": "0.0.2-alpha.2",
    "babel-core": "^6.26.0",
