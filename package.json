    "memory-fs": "^0.4.1",
    "pm2": "^2.10.2",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.3",
    "require-from-string": "^2.0.1",
    "source-map-support": "^0.5.4",
    "url-loader": "^1.0.1",
    "vue-loader": "^14.2.2",
    "vue-ssr-webpack-plugin": "^3.0.0",
    "vue-style-loader": "^4.1.0",
