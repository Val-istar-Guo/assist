{
  "name": "assist",
  "description": "Time management software",
  "version": "0.0.1",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Val-istar-Guo/assist.git"
  },
  "keywords": [
    "time",
    "management"
  ],
  "author": "Val-istar-Guo",
